#Lista de números pares e lista de números ímpares
#PAR = AZUL = #\33[1;34m
#IMPAR = VERMELHO #\33[1,34m+

pares = []
impares = []

for ip in range(10):
    n = int(input("Digite um número: "))
    if n % 2 == 0:
        pares.append()
    else:
        impares.append()

print(f'Os valores pares da lista são: {n[0]}')
print(f'Os valores ímpares da lista são: {n[1]}')
